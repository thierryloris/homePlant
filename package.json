    "eslint-plugin-vitest": "^0.3.21",
    "h3": "^1.10.1",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "nuxt-svgo": "^4.0.0",
    "nuxt-typed-router": "^3.5.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
