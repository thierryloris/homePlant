    "nuxt": "^3.10.0",
    "pinia": "^2.1.7",
    "postcss-custom-properties": "^13.3.4",
    "sharp": "^0.33.2",
    "swiper": "^11.0.6",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.11.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
